{
	"name": "dx-copy",
	"version": "0.0.6",
	"description": "Copy a GitHub repo to another repo interactively or via CLI",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"dx-copy": "dist/index.js"
	},
	"scripts": {
		"build": "tsup",
		"prepublishOnly": "yarn build",
		"dev": "node dist/index.js",
		"dev:watch": "tsup --watch"
	},
	"keywords": [
		"git",
		"github",
		"cli",
		"copy",
		"repository",
		"migration"
	],
	"author": "Md Shahriyar Alam",
	"license": "MIT",
	"dependencies": {
		"enquirer": "^2.4.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.5",
		"@types/node": "^24.10.1",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3"
	}
}
